{
  "name": "threeplot",
  "version": "0.2.10",
  "description": "programmatically create 3d plots with threejs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "targets": {
    "types": false,
    "main": false
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "https://github.com/marnec/threeplot"
  },
  "funding": {
    "type": "patreon",
    "url": "https://patreon.com/marnec"
  },
  "scripts": {
    "dev": "npx tsc --watch",
    "test": "jest",
    "e2e": "npx parcel test/e2e/index.html --dist-dir=docs",
    "build-all": "npx tsc && npx parcel build test/e2e/index.html --dist-dir=docs --public-url https://marnec.github.io/threeplot/",
    "build": "npx tsc"
  },
  "alias": {
    "three": "./node_modules/three"
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "three": "0.158.0"
  },
  "devDependencies": {
    "@types/three": "0.158.2",
    "jest": "^29.7.0",
    "parcel": "^2.10.3",
    "typescript": "5.2.2"
  }
}